{
    "name": "logipro/notifications",
    "type": "app",
    "description": "a Notification micro-service",
    "keywords": ["webpush", "notification"],
    "homepage": "https://github.com/logipro-fr/notifications",
    "license": "Copyrighted",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nathan Ravel",
            "email": "nathan.ravel@logipro.com",
            "homepage": "https://fr.linkedin.com/in/ravelnathan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.3",
        "minishlink/web-push": "^8.0",
        "symfony/http-client": "^7.0",
        "symfony/http-foundation": "^7.1",
        "symfony/framework-bundle": "^7.1",
        "symfony/runtime": "^7.0",
        "symfony/routing": "^7.1",
        "symfony/browser-kit": "^7.0",
        "symfony/orm-pack": "^2.4",
        "symfony/console": "^7.1",
        "phariscope/doctrine-testing-tools": "^1.3",
        "phariscope/event": "^1.1",
        "symfony/config": "^7.1",
        "symfony/http-kernel": "^7.1",
        "symfony/dependency-injection": "^7.1",
        "symfony/monolog-bundle": "^3.10"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.28",
        "phpstan/phpdoc-parser": "^1.28",
        "phpstan/phpstan": "1.10",
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "3.*",
        "behat/behat": "^3.14"
    },
    "autoload": {
        "psr-4" : {
            "Notifications\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Notifications\\Tests\\" : "tests/unit"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "runtime": {
            "dotenv_path": "src/Infrastructure/Shared/Symfony/.env"
        }
    }
}
