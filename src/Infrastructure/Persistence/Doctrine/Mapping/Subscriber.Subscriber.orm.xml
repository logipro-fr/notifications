<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">



    <entity name="Notifications\Domain\Model\Subscriber\Subscriber" 
            table="subscribers"
            repository-class="Notifications\Infrastructure\Persistence\Subscriber\SubscriberRepositoryDoctrine">
        <id name="endpoint" 
            type="endpoint" 
            column="endpoint"/>
        <field  name="expirationTime" 
                type="expirationTime" 
                column="expirationTime"/>
        <field  name="keys" 
                type="keys" 
                column="subscriptionKey"/>
    </entity>
</doctrine-mapping>
